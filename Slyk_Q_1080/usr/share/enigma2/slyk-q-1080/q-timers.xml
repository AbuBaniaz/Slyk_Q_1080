		<widget source="help" position="540,140" size="1002,195" font="Regular;33" foregroundColor="white" backgroundColor="topmid" transparent="1"/>
		<panel name="HelpWindow"/>
		<panel name="VKeyIcon"/>
	</screen>
	<screen name="AutoTimerFAQ" position="0,0" size="1920,1080" title="AutoTimer FAQ" flags="wfNoBorder" backgroundColor="mid">
