						MultiContentEntryText(pos=(15,0), size=(486,36), text = 0, font = 0, flags = RT_HALIGN_LEFT|RT_VALIGN_CENTER), #Event
				                MultiContentEntryText(pos=(501, 0), size=(486,36), text = 3, font = 0, flags = RT_HALIGN_RIGHT|RT_VALIGN_CENTER), #Service
