		<ePixmap position="540,384" size="1002,3" pixmap="div-h.png" alphatest="blend" transparent="1"/>
		<ePixmap position="540,489" size="1002,3" pixmap="div-h.png" alphatest="blend" transparent="1"/>
		<ePixmap position="540,594" size="1002,3" pixmap="div-h.png" alphatest="blend" transparent="1"/>
		<ePixmap position="540,699" size="1002,3" pixmap="div-h.png" alphatest="blend" transparent="1"/>
		<ePixmap position="540,804" size="1002,3" pixmap="div-h.png" alphatest="blend" transparent="1"/>
